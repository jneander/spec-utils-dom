{
  "name": "@jneander/spec-utils-dom",
  "version": "1.1.2",
  "description": "Spec utilities for DOM interactions",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "tsc -p ./config/tsconfig.cjs.json",
    "build:es": "tsc -p ./config/tsconfig.esm.json",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prebuild": "npm run clean",
    "test": "./scripts/test.js",
    "test:head": "npm run test -- --no-headless",
    "test:head:watch": "npm run test -- --no-headless --watch",
    "test:watch": "npm run test -- --watch",
    "types:test": "tsc --noEmit",
    "types:test:watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/spec-utils-dom"
  },
  "keywords": [
    "dom",
    "specs",
    "utils"
  ],
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "release": {
    "extends": [
      "@jneander/semantic-release-config"
    ]
  },
  "dependencies": {
    "@jneander/utils-async": "^2.0.0",
    "@testing-library/dom": "^8.0.0",
    "keycodes": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime-corejs3": "^7.12.13",
    "@jneander/dev-utils-node": "^2.0.0",
    "@jneander/prettier-config": "^1.0.0",
    "@jneander/semantic-release-config": "^1.0.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^9.0.0",
    "chai": "^4.3.0",
    "core-js": "^3.8.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.8.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.0",
    "typescript": "^4.9.3",
    "util": "^0.12.3",
    "webpack": "^5.21.1"
  },
  "prettier": "@jneander/prettier-config"
}
